<div class="container">
  <h2 class="text-center">Websites We're Tracking For You</h2>
  <p class="text-center"><%= link_to 'Add A New Website', new_application_path, class: 'btn btn-success'%></p>

  <div class="container">
    <% if @applications.count == 0 %>
      <%= render partial: 'users/new_user' %>
    <% else %>
      <%= render partial: 'applications/websites' %>
    <% end %>
  </div>
</div>
